{"ast":null,"code":"import \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.for-each.js\";\nexport default {\n  name: 'CriarEquipamento',\n  data() {\n    return {\n      form: {\n        nome: '',\n        marca: '',\n        modelo: '',\n        estado: '',\n        preco: '',\n        loja_id: '',\n        catalogo_id: '',\n        categoria_id: '',\n        tipo_id: ''\n      },\n      imagem: null,\n      categorias: [],\n      // novas listas\n      tipos: [],\n      lojas: [],\n      // 游녣 novo array\n      equipamentoIdApagar: '',\n      equipamentoIdEditar: '',\n      formEditar: {\n        nome: '',\n        preco: ''\n      }\n    };\n  },\n  methods: {\n    handleFileUpload(event) {\n      this.imagem = event.target.files[0];\n    },\n    async submitForm() {\n      try {\n        const formData = new FormData();\n        Object.keys(this.form).forEach(key => {\n          formData.append(key, this.form[key]);\n        });\n        if (this.imagem) {\n          formData.append('imagem', this.imagem);\n        }\n        const response = await fetch('http://localhost:3000/api/equipamentos', {\n          method: 'POST',\n          body: formData\n        });\n        if (!response.ok) {\n          // Tenta ler a mensagem de erro da resposta JSON\n          const errorData = await response.json();\n          if (response.status === 409) {\n            alert(errorData.message || 'Equipamento j치 existe.');\n          } else {\n            alert(errorData.message || 'Erro ao registrar equipamento');\n          }\n          return; // Sai da fun칞칚o para n칚o continuar\n        }\n        const data = await response.json();\n        console.log('Equipamento registrado:', data);\n        alert('Equipamento registrado com sucesso!');\n\n        // Opcional: resetar o formul치rio\n        this.form = {\n          nome: '',\n          marca: '',\n          modelo: '',\n          estado: '',\n          preco: '',\n          loja_id: '',\n          catalogo_id: '',\n          categoria_id: '',\n          tipo_id: ''\n        };\n        this.imagem = null;\n      } catch (error) {\n        console.error('Erro:', error);\n        alert('Erro ao registrar equipamento: ' + error.message);\n      }\n    },\n    async carregarCategoriasETipos() {\n      try {\n        const [catRes, tipoRes, lojaRes] = await Promise.all([fetch('http://localhost:3000/api/categorias'), fetch('http://localhost:3000/api/tipos'), fetch('http://localhost:3000/api/lojas') // 游녣 nova rota\n        ]);\n        this.categorias = await catRes.json();\n        this.tipos = await tipoRes.json();\n        this.lojas = await lojaRes.json(); // 游녣 nova lista\n      } catch (err) {\n        console.error('Erro ao carregar categorias, tipos ou lojas:', err);\n      }\n    }\n  },\n  created() {\n    this.carregarCategoriasETipos();\n  }\n};","map":{"version":3,"names":["name","data","form","nome","marca","modelo","estado","preco","loja_id","catalogo_id","categoria_id","tipo_id","imagem","categorias","tipos","lojas","equipamentoIdApagar","equipamentoIdEditar","formEditar","methods","handleFileUpload","event","target","files","submitForm","formData","FormData","Object","keys","forEach","key","append","response","fetch","method","body","ok","errorData","json","status","alert","message","console","log","error","carregarCategoriasETipos","catRes","tipoRes","lojaRes","Promise","all","err","created"],"sources":["C:\\Users\\heieb\\OneDrive\\Ambiente de Trabalho\\10ABRIL\\Projeto-PTR-PTI\\src\\components\\RegistrarEquipamento.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <header>\r\n      <h1>FromU2Me</h1>\r\n      <nav>\r\n        <ul>\r\n          <li><router-link to=\"/home\">In칤cio</router-link></li>\r\n          <li><a href=\"#produtos\">Produtos</a></li>\r\n          <li><a href=\"#contato\">Contato</a></li>\r\n          \r\n        </ul>\r\n      </nav>\r\n    </header>\r\n\r\n    <section>\r\n      <div id=\"login\">\r\n        <form @submit.prevent=\"submitForm\">\r\n          <div class=\"imgcontainer\">\r\n            <img src=\"/Images/smile.jpg\" alt=\"Avatar\" class=\"avatar\" />\r\n            <h1>Registar Equipamento</h1>\r\n          </div>\r\n\r\n          <div class=\"container\">\r\n            <!-- Nome -->\r\n            <label for=\"nome\">Nome</label>\r\n            <input\r\n              type=\"text\"\r\n              placeholder=\"Nome do equipamento\"\r\n              name=\"nome\"\r\n              v-model=\"form.nome\"\r\n              required\r\n            />\r\n\r\n            <label for=\"estado\">Estado</label>\r\n            <select required v-model=\"form.estado\">\r\n              <option disabled value=\"\">Selecionar estado</option>\r\n              <option value=\"novo\">Novo</option>\r\n              <option value=\"usado\">Usado</option>\r\n              <option value=\"avariado\">Avariado</option>\r\n            </select>\r\n\r\n\r\n            <!-- Marca -->\r\n            <label for=\"marca\">Marca</label>\r\n            <select required id=\"marca\" name=\"marca\" v-model=\"form.marca\">\r\n              <option disabled value=\"\">Selecione uma marca</option>\r\n              <option value=\"Xiaomi\">Xiaomi</option>\r\n              <option value=\"Nokia\">Nokia</option>\r\n              <option value=\"Outra\">Outra</option>\r\n            </select>\r\n\r\n            <!-- Modelo -->\r\n            <label for=\"modelo\"><b>Modelo</b></label>\r\n            <input\r\n              type=\"text\"\r\n              placeholder=\"Introduza um modelo\"\r\n              name=\"modelo\"\r\n              v-model=\"form.modelo\"\r\n              required\r\n            />\r\n\r\n\r\n            <!-- Adiciona depois do campo cat치logo_id -->\r\n            <label for=\"imagem\"><b>Imagem</b></label>\r\n            <input\r\n              type=\"file\"\r\n              name=\"imagem\"\r\n              accept=\"image/*\"\r\n              @change=\"handleFileUpload\"\r\n            />\r\n\r\n\r\n            <!-- Pre칞o -->\r\n            <label for=\"preco\"><b>Pre칞o</b></label>\r\n            <input\r\n              type=\"number\"\r\n              placeholder=\"Introduza um pre칞o\"\r\n              name=\"preco\"\r\n              v-model=\"form.preco\"\r\n              required\r\n            />\r\n\r\n            <!-- Loja ID -->\r\n            <!-- Loja -->\r\n            <label for=\"loja_id\"><b>Loja</b></label>\r\n            <select v-model=\"form.loja_id\" required>\r\n              <option disabled value=\"\">Selecione uma loja</option>\r\n              <option v-for=\"loja in lojas\" :key=\"loja._id\" :value=\"loja._id\">\r\n                {{ loja.nome }}\r\n              </option>\r\n            </select>\r\n\r\n\r\n\r\n            <!-- Cat치logo ID -->\r\n            <label for=\"catalogo_id\"><b>ID do Cat치logo</b></label>\r\n            <input\r\n              type=\"text\"\r\n              placeholder=\"ID do cat치logo de equipamentos\"\r\n              name=\"catalogo_id\"\r\n              v-model=\"form.catalogo_id\"\r\n              required\r\n            />\r\n\r\n\r\n           <!-- Categoria -->\r\n          <label for=\"categoria_id\"><b>Categoria</b></label>\r\n          <select v-model=\"form.categoria_id\" required>\r\n            <option disabled value=\"\">Selecione uma categoria</option>\r\n            <option v-for=\"cat in categorias\" :key=\"cat._id\" :value=\"cat._id\">\r\n              {{ cat.nome }}\r\n            </option>\r\n          </select>\r\n\r\n          <!-- Tipo -->\r\n          <label for=\"tipo_id\"><b>Tipo</b></label>\r\n          <select v-model=\"form.tipo_id\" required>\r\n            <option disabled value=\"\">Selecione um tipo</option>\r\n            <option v-for=\"tipo in tipos\" :key=\"tipo._id\" :value=\"tipo._id\">\r\n              {{ tipo.nome }}\r\n            </option>\r\n          </select>\r\n\r\n\r\n\r\n            <button type=\"submit\">Registrar Equipamento</button>\r\n          </div>\r\n        </form>\r\n\r\n\r\n        <!-- Apagar Equipamento -->\r\n        <div class=\"container\">\r\n          <h2>Apagar Equipamento</h2>\r\n          <label for=\"equipamento_id_apagar\">ID do Equipamento</label>\r\n          <input\r\n            type=\"text\"\r\n            placeholder=\"Insira o ID do equipamento\"\r\n            v-model=\"equipamentoIdApagar\"\r\n          />\r\n          <button @click=\"apagarEquipamento\">Apagar Equipamento</button>\r\n        </div>\r\n\r\n        <!-- Modificar Equipamento -->\r\n        <div class=\"container\">\r\n          <h2>Modificar Equipamento</h2>\r\n\r\n          <label for=\"equipamento_id_editar\">ID do Equipamento</label>\r\n          <input\r\n            type=\"text\"\r\n            placeholder=\"ID do equipamento a editar\"\r\n            v-model=\"equipamentoIdEditar\"\r\n          />\r\n\r\n          <label for=\"novo_nome\">Novo Nome</label>\r\n          <input type=\"text\" v-model=\"formEditar.nome\" />\r\n\r\n          <label for=\"novo_preco\">Novo Pre칞o</label>\r\n          <input type=\"number\" v-model=\"formEditar.preco\" />\r\n\r\n          <button @click=\"editarEquipamento\">Salvar Altera칞칫es</button>\r\n      </div>\r\n    </div>\r\n\r\n    </section>\r\n\r\n    <footer>\r\n      <p>&copy; 2025 Loja Tech - Todos os direitos reservados.</p>\r\n    </footer>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: 'CriarEquipamento',\r\n  data() {\r\n    return {\r\n      form: {\r\n        nome: '',\r\n        marca: '',\r\n        modelo: '',\r\n        estado: '',\r\n        preco: '',\r\n        loja_id: '',\r\n        catalogo_id: '',\r\n        categoria_id: '',\r\n        tipo_id: ''\r\n      },\r\n      imagem: null,\r\n      categorias: [], // novas listas\r\n      tipos: [],\r\n      lojas: [],  // 游녣 novo array\r\n      equipamentoIdApagar: '',\r\n      equipamentoIdEditar: '',\r\n      formEditar: {\r\n        nome: '',\r\n        preco: ''\r\n      },\r\n\r\n    };\r\n  },\r\n  methods: {\r\n    handleFileUpload(event) {\r\n      this.imagem = event.target.files[0];\r\n    },\r\n\r\n    async submitForm() {\r\n  try {\r\n    const formData = new FormData();\r\n    Object.keys(this.form).forEach(key => {\r\n      formData.append(key, this.form[key]);\r\n    });\r\n    if (this.imagem) {\r\n      formData.append('imagem', this.imagem);\r\n    }\r\n\r\n    const response = await fetch('http://localhost:3000/api/equipamentos', {\r\n      method: 'POST',\r\n      body: formData\r\n    });\r\n\r\n    if (!response.ok) {\r\n      // Tenta ler a mensagem de erro da resposta JSON\r\n      const errorData = await response.json();\r\n      if (response.status === 409) {\r\n        alert(errorData.message || 'Equipamento j치 existe.');\r\n      } else {\r\n        alert(errorData.message || 'Erro ao registrar equipamento');\r\n      }\r\n      return; // Sai da fun칞칚o para n칚o continuar\r\n    }\r\n\r\n    const data = await response.json();\r\n    console.log('Equipamento registrado:', data);\r\n    alert('Equipamento registrado com sucesso!');\r\n\r\n    // Opcional: resetar o formul치rio\r\n    this.form = {\r\n      nome: '',\r\n      marca: '',\r\n      modelo: '',\r\n      estado: '',\r\n      preco: '',\r\n      loja_id: '',\r\n      catalogo_id: '',\r\n      categoria_id: '',\r\n      tipo_id: ''\r\n    };\r\n    this.imagem = null;\r\n\r\n  } catch (error) {\r\n    console.error('Erro:', error);\r\n    alert('Erro ao registrar equipamento: ' + error.message);\r\n  }\r\n}\r\n,\r\n\r\n    async carregarCategoriasETipos() {\r\n      try {\r\n        const [catRes, tipoRes, lojaRes] = await Promise.all([\r\n          fetch('http://localhost:3000/api/categorias'),\r\n          fetch('http://localhost:3000/api/tipos'),\r\n          fetch('http://localhost:3000/api/lojas') // 游녣 nova rota\r\n        ]);\r\n\r\n        this.categorias = await catRes.json();\r\n        this.tipos = await tipoRes.json();\r\n        this.lojas = await lojaRes.json(); // 游녣 nova lista\r\n      } catch (err) {\r\n        console.error('Erro ao carregar categorias, tipos ou lojas:', err);\r\n      }\r\n    }\r\n\r\n  },\r\n\r\n  created() {\r\n    this.carregarCategoriasETipos();\r\n  }\r\n};\r\n\r\n\r\n</script>\r\n\r\n\r\n  \r\n  <style scoped>\r\n  header {\r\n    background: #0d6efd;\r\n    color: #fff;\r\n    padding: 20px;\r\n    text-align: center;\r\n  }\r\n  \r\n  .imgcontainer {\r\n    text-align: center;\r\n    margin: 20px;\r\n  }\r\n  \r\n  .container {\r\n    display: flex;\r\n    flex-direction: column;\r\n    gap: 10px;\r\n    padding: 20px;\r\n    max-width: 400px;\r\n    margin: auto;\r\n  }\r\n  \r\n  button {\r\n    background: #0d6efd;\r\n    color: white;\r\n    padding: 10px;\r\n    border: none;\r\n    cursor: pointer;\r\n    font-weight: bold;\r\n  }\r\n  \r\n  button:hover {\r\n    background: #084298;\r\n  }\r\n  body {\r\n    font-family: 'Poppins', sans-serif;\r\n    margin: 0;\r\n    padding: 0;\r\n    background-color: #f8f9fa;\r\n    color: #333;\r\n}\r\nheader {\r\n    background: #0d6efd;\r\n    color: #fff;\r\n    padding: 20px;\r\n    text-align: center;\r\n    box-shadow: 0 4px 6px rgba(0, 0, 0, 0.1);\r\n}\r\nnav ul {\r\n    list-style: none;\r\n    padding: 0;\r\n    display: flex;\r\n    justify-content: center;\r\n    gap: 20px;\r\n}\r\nnav ul li {\r\n    display: inline;\r\n}\r\nnav ul li a {\r\n    color: #fff;\r\n    text-decoration: none;\r\n    font-weight: bold;\r\n}\r\n.banner {\r\n    text-align: center;\r\n    padding: 60px 20px;\r\n    background: linear-gradient(to right, #0d6efd, #6610f2);\r\n    color: #fff;\r\n}\r\n.produtos {\r\n    display: grid;\r\n    grid-template-columns: repeat(auto-fit, minmax(300px, 1fr));\r\n    gap: 20px;\r\n    padding: 40px;\r\n    max-width: 1200px;\r\n    margin: 0 auto;\r\n    \r\n}\r\n\r\n\r\nfooter {\r\n    background: #0d6efd;\r\n    color: #fff;\r\n    text-align: center;\r\n    padding: 20px;\r\n 